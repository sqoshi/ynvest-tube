<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.app.ynvest_tube">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ynvest_tube_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ynvest_tube_logo_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.YnvestTube"
        android:usesCleartextTraffic="true">
        <activity android:name="com.app.ynvest_tube.activities.AuctionActivity" />
        <activity android:name="com.app.ynvest_tube.activities.NavigationActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>